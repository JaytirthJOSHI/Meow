{
  "scopeName": "source.meow",
  "patterns": [
    {
      "name": "comment.line.meow",
      "match": "^\\s*🐾.*🐾"
    },
    {
      "name": "keyword.control.meow",
      "match": "\\b(meow|hiss|purr|nap|scratch|lick|zoomies|yowl|paw|sleep|chase|mew|stretch|pounce)\\b"
    }
  ],
  "fileTypes": ["meow"],
  "name": "MeowLang"
}